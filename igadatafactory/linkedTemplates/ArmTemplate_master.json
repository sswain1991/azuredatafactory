{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "igadatafactory"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureMySql_1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureMySql_1'"
		},
		"AzureSqlDatabaseiga_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabaseiga'"
		},
		"SqlServer1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServer1'"
		},
		"SqlServer1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SqlServer1'"
		},
		"igaservicenow_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'igaservicenow'"
		},
		"AzureDataLakeStore_properties_typeProperties_dataLakeStoreUri": {
			"type": "string",
			"defaultValue": "https://igadatalake.azuredatalakestore.net/webhdfs/v1"
		},
		"AzureDataLakeStore_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "9f58333b-9cca-4bd9-a7d8-e151e43b79f3"
		},
		"AzureDataLakeStore_properties_typeProperties_subscriptionId": {
			"type": "string",
			"defaultValue": "58334b57-3447-4951-8505-9a3f7873c5ba"
		},
		"AzureDataLakeStore_properties_typeProperties_resourceGroupName": {
			"type": "string",
			"defaultValue": "AzureIGA-rg"
		},
		"SqlServer1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Administrator"
		},
		"igaservicenow_properties_typeProperties_endpoint": {
			"type": "string",
			"defaultValue": "https://dev91871.service-now.com"
		},
		"igaservicenow_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "admin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "igadatafactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureMySql_1_connectionString": {
						"value": "[parameters('AzureMySql_1_connectionString')]"
					},
					"AzureSqlDatabaseiga_connectionString": {
						"value": "[parameters('AzureSqlDatabaseiga_connectionString')]"
					},
					"SqlServer1_connectionString": {
						"value": "[parameters('SqlServer1_connectionString')]"
					},
					"SqlServer1_password": {
						"value": "[parameters('SqlServer1_password')]"
					},
					"igaservicenow_password": {
						"value": "[parameters('igaservicenow_password')]"
					},
					"AzureDataLakeStore_properties_typeProperties_dataLakeStoreUri": {
						"value": "[parameters('AzureDataLakeStore_properties_typeProperties_dataLakeStoreUri')]"
					},
					"AzureDataLakeStore_properties_typeProperties_tenant": {
						"value": "[parameters('AzureDataLakeStore_properties_typeProperties_tenant')]"
					},
					"AzureDataLakeStore_properties_typeProperties_subscriptionId": {
						"value": "[parameters('AzureDataLakeStore_properties_typeProperties_subscriptionId')]"
					},
					"AzureDataLakeStore_properties_typeProperties_resourceGroupName": {
						"value": "[parameters('AzureDataLakeStore_properties_typeProperties_resourceGroupName')]"
					},
					"SqlServer1_properties_typeProperties_userName": {
						"value": "[parameters('SqlServer1_properties_typeProperties_userName')]"
					},
					"igaservicenow_properties_typeProperties_endpoint": {
						"value": "[parameters('igaservicenow_properties_typeProperties_endpoint')]"
					},
					"igaservicenow_properties_typeProperties_username": {
						"value": "[parameters('igaservicenow_properties_typeProperties_username')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "igadatafactory_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'igadatafactory_ArmTemplate_0')]"
			]
		}
	]
}